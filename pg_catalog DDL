	xid BIGINT NOT NULL ENCODE lzo,
	seq INTEGER NOT NULL ENCODE lzo,
	repository_name CHAR(32) NOT NULL ENCODE lzo,
	backup_name CHAR(256) NOT NULL ENCODE lzo,
	backup_type CHAR(12) NOT NULL ENCODE lzo,
	starttime TIMESTAMP NOT NULL ENCODE lzo,
	num_tables INTEGER NOT NULL ENCODE lzo,
	num_tables_complete INTEGER NOT NULL ENCODE lzo,
	est_pct_complete INTEGER NOT NULL ENCODE lzo,
	est_time_complete TIMESTAMP NOT NULL ENCODE lzo
)
DISTSTYLE EVEN;


